  },
  "devDependencies": {
    "@typechain/ethers-v5": "^6.0.2",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "chai": "^4.3.3",
    "ethereum-waffle": "^3.3.0",
    "mocha": "^8.3.1",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.3",
    "typechain-target-ethers-v5": "^5.0.1",
    "typescript": "^4.2.3"
  }
}
